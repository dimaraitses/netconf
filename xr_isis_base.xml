<config xmlns:xc="urn:ietf:params:xml:ns:netconf:base:1.0">
  <isis xmlns="http://cisco.com/ns/yang/Cisco-IOS-XR-clns-isis-cfg">
   <instances>
    <instance>
     <instance-name>CORE</instance-name>
     <running></running>
     <overload-bits>
      <overload-bit>
       <level>not-set</level>
       <overload-bit-mode>startup-period</overload-bit-mode>
       <hippity-period>60</hippity-period>
      </overload-bit>
     </overload-bits>
     <is-type>level2</is-type>
     <nets>
      <net>
       <net-name>{{ NET }}</net-name>
      </net>
     </nets>
     <log-adjacency-changes></log-adjacency-changes>
     <lsp-refresh-intervals>
      <lsp-refresh-interval>
       <level>not-set</level>
       <interval>65000</interval>
      </lsp-refresh-interval>
     </lsp-refresh-intervals>
     <lsp-lifetimes>
      <lsp-lifetime>
       <level>not-set</level>
       <lifetime>65535</lifetime>
      </lsp-lifetime>
     </lsp-lifetimes>
     <afs>
      <af>
       <af-name>ipv4</af-name>
       <saf-name>unicast</saf-name>
       <af-data>
        <metric-styles>
         <metric-style>
          <level>not-set</level>
          <style>new-metric-style</style>
          <transition-state>disabled</transition-state>
         </metric-style>
        </metric-styles>
        <spf-intervals>
         <spf-interval>
          <level>not-set</level>
          <maximum-wait>5000</maximum-wait>
          <initial-wait>150</initial-wait>
          <secondary-wait>300</secondary-wait>
         </spf-interval>
        </spf-intervals>
       </af-data>
      </af>
      <af>
       <af-name>ipv6</af-name>
       <saf-name>unicast</saf-name>
       <af-data>
        <metric-styles>
         <metric-style>
          <level>not-set</level>
          <style>new-metric-style</style>
          <transition-state>disabled</transition-state>
         </metric-style>
        </metric-styles>
        <spf-intervals>
         <spf-interval>
          <level>not-set</level>
          <maximum-wait>5000</maximum-wait>
          <initial-wait>150</initial-wait>
          <secondary-wait>300</secondary-wait>
         </spf-interval>
        </spf-intervals>
       </af-data>
      </af>
     </afs>
     <interfaces>
      <interface>
       <interface-name>Loopback0</interface-name>
       <running></running>
       <interface-afs>
        <interface-af>
         <af-name>ipv4</af-name>
         <saf-name>unicast</saf-name>
         <interface-af-data>
          <running></running>
         </interface-af-data>
        </interface-af>
        <interface-af>
         <af-name>ipv6</af-name>
         <saf-name>unicast</saf-name>
         <interface-af-data>
          <running></running>
         </interface-af-data>
        </interface-af>
       </interface-afs>
      </interface>
      <interface>
       <interface-name>GigabitEthernet0/0/0/0</interface-name>
       <running></running>
       <point-to-point></point-to-point>
       <interface-afs>
        <interface-af>
         <af-name>ipv4</af-name>
         <saf-name>unicast</saf-name>
         <interface-af-data>
          <running></running>
         </interface-af-data>
        </interface-af>
        <interface-af>
         <af-name>ipv6</af-name>
         <saf-name>unicast</saf-name>
         <interface-af-data>
          <running></running>
         </interface-af-data>
        </interface-af>
       </interface-afs>
      </interface>
      <interface>
       <interface-name>GigabitEthernet0/0/0/1</interface-name>
       <running></running>
       <interface-afs>
        <interface-af>
         <af-name>ipv4</af-name>
         <saf-name>unicast</saf-name>
         <interface-af-data>
          <running></running>
          <metrics>
           <metric>
            <level>not-set</level>
            <metric>20</metric>
           </metric>
          </metrics>
         </interface-af-data>
        </interface-af>
        <interface-af>
         <af-name>ipv6</af-name>
         <saf-name>unicast</saf-name>
         <interface-af-data>
          <running></running>
          <metrics>
           <metric>
            <level>not-set</level>
            <metric>20</metric>
           </metric>
          </metrics>
         </interface-af-data>
        </interface-af>
       </interface-afs>
      </interface>
     </interfaces>
    </instance>
   </instances>
  </isis>
</config>